    "@trpc/server": "^10.38.3",
    "@types/node": "^20.3.1",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "eslint": "^8.42.0",
    "eslint-config-next": "13.4.19",
    "next": "13.4.19",
    "postcss": "8.4.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^1.13.1",
    "tailwindcss": "3.3.3",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0"
  }
